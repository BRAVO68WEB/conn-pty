{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "pkg/**",
        "packages/console/dist/**",
        "packages/api/dist/**",
        "packages/ssh-client-wasm/pkg/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "start": {
      "dependsOn": ["build"]
    }
  }
}